<div class="col-md-4">
  {{#if model.songs}}
    <h2>List of songs for {{model.artist.artistName}}</h2>
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Title</th>
          <th>Price</th>
          <th>Play count</th>
        </tr>
      </thead>
      <tbody>
        {{#each model.songs as |song|}}
          <tr>
              <td>{{song.title}}</td>
              <td>{{song.price}}</td>
              <td>{{song.playCount}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  {{else}}
    <h2>No songs for {{model.artist.artistName}}</h2>
  {{/if}}
</div>
{{outlet}}
